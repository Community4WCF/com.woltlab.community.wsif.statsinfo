<data>
	<import>
		<categories>
			<category name="filebase.index.statsinfo">
				<parent>filebase.index</parent>
			</category>
		</categories>
		<options>
			<!-- index.general -->
			<option name="index_statsinfo_enable">
				<categoryname>filebase.index.statsinfo</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
				<enableoptions><![CDATA[index_statsinfo_link_enable,index_statsinfo_link]]></enableoptions>
			</option>
			<option name="index_statsinfo_link_enable">
				<categoryname>filebase.index.statsinfo</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>0</defaultvalue>
				<enableoptions><![CDATA[index_statsinfo_link]]></enableoptions>
			</option>
			<option name="index_statsinfo_link">
				<categoryname>filebase.index.statsinfo</categoryname>
				<optiontype>text</optiontype>
				<defaultvalue>index.php?page=Index</defaultvalue>
			</option>
			<!-- /index.general -->
		</options>
	</import>
	<delete>
		<option name="index_enable_statsinfo"></option>
	</delete>
</data>
